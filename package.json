{
  "name": "@umm/git",
  "version": "2.0.1",
  "description": "git command wrapper for Unity",
  "homepage": "https://github.com/umm-projects/git",
  "bugs": {
    "url": "https://github.com/umm-projects/git/issues"
  },
  "scripts": {
    "umm:init": "./node_modules/.bin/umm-init",
    "umm:install": "./node_modules/.bin/umm-install",
    "umm:uninstall": "./node_modules/.bin/umm-uninstall",
    "postinstall": "npm run --silent umm:install",
    "postuninstall": "npm run --silent umm:uninstall",
    "build:test": "tar -tf $(npm pack -s) && $(npm bin)/rimraf $_"
  },
  "author": "Tetsuya Mori",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:umm-projects/git"
  },
  "files": [
    "Assets",
    "scripts"
  ],
  "dependencies": {
    "@umm/command_runner": "umm-projects/command_runner#^1.0.0",
    "@umm/environment_setting": "umm-projects/environment_setting#^1.0.0",
    "@umm/scripts": "umm-projects/scripts#^1.0.0",
    "@umm/unirx": "umm-projects/unirx#^1.0.0"
  },
  "devDependencies": {}
}
